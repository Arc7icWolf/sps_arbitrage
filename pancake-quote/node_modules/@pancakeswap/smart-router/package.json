{
  "name": "@pancakeswap/smart-router",
  "version": "7.5.2",
  "description": "A SDK for getting best routes from AMM",
  "repository": {
    "type": "git",
    "url": "https://github.com/pancakeswap/pancake-frontend.git",
    "directory": "packages/smart-router"
  },
  "main": "dist/evm.js",
  "module": "dist/evm.mjs",
  "types": "dist/evm/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "dependencies": {
    "async-retry": "^1.3.1",
    "debug": "^4.3.4",
    "graphql": "^16.8.1",
    "graphql-request": "5.0.0",
    "lodash": "^4.17.21",
    "mnemonist": "^0.38.3",
    "stats-lite": "^2.2.0",
    "tiny-invariant": "^1.3.0",
    "viem": "2.31.3",
    "zod": "^3.25.67",
    "@pancakeswap/chains": "0.6.0",
    "@pancakeswap/multicall": "3.7.2",
    "@pancakeswap/sdk": "5.8.16",
    "@pancakeswap/stable-swap-sdk": "2.0.9",
    "@pancakeswap/swap-sdk-core": "1.5.0",
    "@pancakeswap/token-lists": "0.0.16",
    "@pancakeswap/tokens": "0.7.8",
    "@pancakeswap/v3-sdk": "3.9.5",
    "@pancakeswap/infinity-sdk": "1.0.5"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.4",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.178",
    "@types/stats-lite": "^2.2.0",
    "tsup": "^6.7.0",
    "quick-lru": "7.0.0",
    "@pancakeswap/tsconfig": "0.0.0",
    "@pancakeswap/utils": "8.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/evm/index.d.ts",
      "import": "./dist/evm.mjs",
      "require": "./dist/evm.js"
    },
    "./evm": {
      "types": "./dist/evm/index.d.ts",
      "import": "./dist/evm.mjs",
      "require": "./dist/evm.js"
    },
    "./legacy-router": {
      "types": "./dist/legacy-router/index.d.ts",
      "import": "./dist/legacy-router.mjs",
      "require": "./dist/legacy-router.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "update:snapshot": "vitest -u",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typechecks": "tsc --noEmit -p ./tsconfig.json"
  }
}